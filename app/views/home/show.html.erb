  <% if current_user %>
  <h3>Logged in as <%= current_user.email_address %></h3>
  <h4><%= link_to "My Links", links_path %></h4>
  <h4><%= link_to "Logout", logout_path, method: :delete %></h4>
  <% else %>
  <h4><%= link_to 'Login', login_path %></h4>
  <h4><%= link_to 'Create New Account', new_user_path %></h4>
  <% end %>
